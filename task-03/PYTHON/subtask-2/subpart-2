input_file_path = '/home/albustroke/amfoss-task/task-03/PYTHON/subtask-1/input.txt'
output_file_path = '/home/albustroke/amfoss-task/task-03/PYTHON/subtask-1/output.txt'
with open(input_file_path, 'r') as input_file:
        
        content = input_file.read()

with open(output_file_path, 'w') as output_file:
        output_file.write(content)

print("Successfully copied from input.txt to output.txt.")
